{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "analyze": "tsc -b && vite build --mode analyze",
    "fix": "oxlint --fix --fix-suggestions --fix-dangerously",
    "build:lib": "tsc -b && vite build --mode lib",
    "scss": "sass ./src/assets/styles/lib-imports.scss ./static/app.css  --no-source-map -w",
    "build:app": "rollup -c",
    "unocss": "unocss -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"sass,unocss\" -c \"yellow,cyan\" \"npm run scss\" \"npm run unocss\"",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@douyinfe/semi-icons": "^2.86.0",
    "@douyinfe/semi-illustrations": "^2.86.0",
    "@douyinfe/semi-ui": "^2.86.0",
    "@emotion/styled": "^11.14.1",
    "@r2wc/react-to-web-component": "^2.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@semi-bot/semi-theme-reman-purple": "^1.0.1",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@tanstack/react-query": "^5.85.6",
    "@tanstack/react-router": "^1.131.31",
    "@unocss/preset-rem-to-px": "^66.5.0",
    "@visactor/react-vchart": "^1.13.19",
    "@visactor/vchart-semi-theme": "^1.12.2",
    "axios": "^1.11.0",
    "classnames": "^2.5.1",
    "clipboard": "^2.0.11",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.18",
    "esbuild": "^0.23.1",
    "js-cookie": "^3.0.5",
    "js-md5": "^0.8.3",
    "ky": "^1.9.1",
    "parse-url": "^9.2.0",
    "prettier": "^3.6.2",
    "query-string": "^9.2.2",
    "radash": "^12.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use": "^17.6.0",
    "rollup": "^4.50.0",
    "unocss": "^66.5.0",
    "unocss-preset-animations": "^1.2.1",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-icons": "^22.2.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.380",
    "@tanstack/router-devtools": "^1.131.31",
    "@tanstack/router-plugin": "^1.131.31",
    "@types/js-cookie": "^3.0.6",
    "@types/js-md5": "^0.7.2",
    "@types/node": "^22.18.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@unocss/preset-icons": "^66.5.0",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "concurrently": "^8.2.2",
    "globals": "^15.15.0",
    "less": "^4.4.1",
    "oxlint": "^1.14.0",
    "sass": "^1.91.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-bundle-analyzer": "^0.10.6",
    "vite-plugin-compression2": "^2.2.1",
    "vite-plugin-semi-theming": "^0.1.0",
    "zod": "^3.25.76"
  }
}
