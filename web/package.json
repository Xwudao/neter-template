{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "analyze": "tsc -b && vite build --mode analyze",
    "build:lib": "tsc -b && vite build --mode lib",
    "scss": "sass ./src/assets/styles/lib-imports.scss ./static/app.css  --no-source-map -w",
    "build:app": "rollup -c",
    "unocss": "unocss -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"sass,unocss\" -c \"yellow,cyan\" \"npm run scss\" \"npm run unocss\"",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@douyinfe/semi-icons": "^2.63.1",
    "@douyinfe/semi-ui": "^2.80.0",
    "@emotion/styled": "^11.13.0",
    "@r2wc/react-to-web-component": "^2.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@tanstack/react-query": "^5.51.23",
    "@tanstack/react-router": "^1.50.1",
    "@unocss/preset-rem-to-px": "^0.61.9",
    "@visactor/react-vchart": "^1.13.10",
    "@visactor/vchart-semi-theme": "^1.12.2",
    "axios": "^1.7.4",
    "classnames": "^2.5.1",
    "clipboard": "^2.0.11",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "esbuild": "^0.23.0",
    "js-cookie": "^3.0.5",
    "js-md5": "^0.8.3",
    "ky": "^1.7.1",
    "parse-url": "^9.2.0",
    "prettier": "^3.3.3",
    "query-string": "^9.1.0",
    "radash": "^12.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use": "^17.6.0",
    "rollup": "^4.20.0",
    "unocss": "^66.1.2",
    "unocss-preset-animations": "^1.0.2",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-icons": "^22.1.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
